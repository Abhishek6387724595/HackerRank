#include <bits/stdc++.h>
#include <algorithm>
#include <string>
using namespace std;

int main()
{
    string s;
    getline(cin, s);

    int l = s.length();
    int count=1;
    for(int i=0;i<l;i++)
    {
        if(isupper(s[i]))
        {
            count++;
        }
    }
    cout<<count;
   

    return 0;
}
