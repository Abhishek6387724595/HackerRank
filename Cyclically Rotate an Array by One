#include <bits/stdc++.h> 
using namespace std; 

int main(){

    int arr[]={1,2,3,4,5};

    //int brr[]={};
    int n = sizeof(arr)/sizeof(arr[0]); 

    int temp=arr[n-1];

    // for(int j=0;j<n-1;j++)
    // {
    //    brr[j+1]=arr[j];
    // }

    for(int i = n - 1; i > 0; i--)
    {
        arr[i] = arr[i - 1];
    }
    arr[0]=temp;

    for(int j=0;j<n;j++)
    {
        cout<<" "<<arr[j];
    }
return 0; 
}
