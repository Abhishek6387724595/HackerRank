#include <bits/stdc++.h> 
using namespace std; 

int main(){

    int arr[]={1,2,3,4,5};


    int brr[]={0};
    int n = sizeof(arr)/sizeof(arr[0]); 

    int temp=arr[n-1];

    for(int i=0;i<n-1;i++)
    {
       
        brr[i+1]=arr[i];
    }
    
    brr[0]=temp;

    for(int j=0;j<n;j++)
    {
        cout<<" "<<brr[j];
    }


return 0; 
}
