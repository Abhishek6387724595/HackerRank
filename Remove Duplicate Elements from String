#include<iostream>
using namespace std;

int main(){

    string s,result="";

    cout<<"Enter a string :";
    //cin>>s;
    getline(cin,s);

    int hash[123]={0};

    for(int i=0;i<s.length();i++)
    {
        if(hash[s[i]]==0)
        {cout<<hash[s[1]]<<endl;
            hash[s[i]]=1;
            result+=s[i];
        }
    }

    cout<<result<<endl;
    return 0;
    
}
